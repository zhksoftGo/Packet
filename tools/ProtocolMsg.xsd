<?xml version="1.0" encoding="UTF-8"?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">	<xs:include schemaLocation="Types.xsd"/>	<xs:element name="Method">		<xs:complexType>			<xs:sequence>				<xs:element name="Param" minOccurs="0" maxOccurs="unbounded">					<xs:complexType>						<xs:attribute name="Name" type="xs:string" use="required"/>						<xs:attribute name="Type" type="TypeRange" use="required"/>						<xs:attribute name="Notes" type="xs:string" use="required"/>					</xs:complexType>				</xs:element>			</xs:sequence>			<xs:attribute name="Interval" type="xs:int" use="optional"/>			<xs:attribute name="Name" type="xs:string" use="required"/>			<xs:attribute name="Notes" type="xs:string" use="required"/>		</xs:complexType>	</xs:element>	<xs:element name="FrequencyProtect">		<xs:complexType>			<xs:attribute name="Enable" type="xs:boolean" use="required"/>			<xs:attribute name="Limit" type="xs:int" use="required"/>			<xs:attribute name="TimeStampFunction" type="xs:string" use="required"/>			<xs:attribute name="Notes" type="xs:string" use="required"/>		</xs:complexType>	</xs:element>	<xs:element name="FromServer">		<xs:complexType>			<xs:choice minOccurs="0" maxOccurs="unbounded">				<xs:element ref="Method"/>			</xs:choice>		</xs:complexType>	</xs:element>	<xs:element name="FromClient">		<xs:complexType>			<xs:choice minOccurs="0" maxOccurs="unbounded">				<xs:element ref="Method"/>			</xs:choice>		</xs:complexType>	</xs:element>	<xs:element name="ProtocolMsg">		<xs:complexType>			<xs:choice minOccurs="0" maxOccurs="unbounded">				<xs:element ref="Struct"/>				<xs:element ref="Enum"/>				<xs:element ref="Map"/>				<xs:element ref="Vector"/>				<xs:element ref="Dependences"/>				<xs:element ref="ProtocolTypeDefine"/>				<xs:element ref="FrequencyProtect"/>				<xs:element ref="FromClient"/>				<xs:element ref="FromServer"/>			</xs:choice>			<xs:attribute name="NameSpace" type="xs:string" use="required"/>			<xs:attribute name="GenerateTypeDefine" type="xs:boolean" use="optional"/>			<xs:attribute name="ForNetworkSend" type="xs:boolean" use="optional"/>      <xs:attribute name="NamespaceExt" type="xs:boolean" use="optional"/>    </xs:complexType>	</xs:element></xs:schema>